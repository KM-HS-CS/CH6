6.30
int i = 1;
	while (i <= 12) {
		System.out.print("*");
		if (i % 4 == 0) {
			System.out.println();
		}
		i++;
	}

6.32
Random generator = new Random();
	int friend = generator.nextInt(15);
	if (friend < 10) {
		System.out.println("California");
	}
	else if (friend < 13) {
		System.out.println("Nevada");
	}
	else {
		System.out.println("Utah");
		}
	}}
